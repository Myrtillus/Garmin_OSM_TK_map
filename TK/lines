highway=path & mtb:scale="0+" [0x10E18 road_class=0 resolution 18-22]
highway=path & mtb:scale=1 [0x10E03 road_class=0 resolution 23-24 continue]
highway=path & mtb:scale=1 [0x10E19 road_class=0 resolution 18-22]
highway=path & mtb:scale=2 [0x10E04 road_class=0 resolution 23-24 continue]
highway=path & mtb:scale=2 [0x10E1A road_class=0 resolution 18-22]
