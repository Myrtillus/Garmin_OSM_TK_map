highway=path & mtb:scale=3 [0x10E1B road_class=0 resolution 18-22]
highway=path & mtb:scale=4 [0x10E06 road_class=0 resolution 23-24 continue]
highway=path & mtb:scale=4 [0x10E1C road_class=0 resolution 18-22]
highway=path & mtb:scale=5 [0x10E07 road_class=0 resolution 23-24 continue]
highway=path & mtb:scale=5 [0x10E1D road_class=0 resolution 18-22]

