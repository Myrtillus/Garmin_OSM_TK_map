# -------------------------------------------------------------------
# This is the default mkgmap style file for converting OSM ways
# into Garmin lines.
# Any line prefaced with # is a comment and will not be applied.
# Each line is a rule, of the generalised format:
#  OSM_tag(s)_to_match  [Garmin_type_code resolution]
# See http://wiki.openstreetmap.org/wiki/Mkgmap/help/style_rules
# and http://wiki.openstreetmap.org/wiki/Mkgmap/help/Custom_styles
# for more information.



###################################################################
### Spellcheck
###################################################################

### Convert tag combinations to suitable forms, adapted from OpenMTBMap
highway=service_footway {set highway=footway}
highway=footpath {set highway=footway}
highway="track; footway" {set highway=footway}
highway="track;footway" {set highway=footway}
highway="path;track" {set highway=path}
highway="path; track" {set highway=path}
highway="track; path" {set highway=track}
highway="track;path" {set highway=track}
highway="footway; track" {set highway=footway}
highway="footway;track" {set highway=footway}
highway="residential; track" {set highway=track}
highway="residential;track" {set highway=track}
highway=residental {set highway=residential}
highway="footway; cycleway" {set highway=cycleway; set segregated=no}
highway="footway;cycleway" {set highway=cycleway; set segregated=no}
highway="cycleway; footway" {set highway=cycleway; set segregated=no}
highway="cycleway;footway" {set highway=cycleway; set segregated=no}
highway=cycleroad {set highway=cycleway}
highway=cyclestreet {set highway=cycleway}
bicycle=nope {set bicycle=no}

### Convert used tags to lowercase
Highway=* & highway!=* {set highway='${Highway}'}
HighWay=* & highway!=* {set highway='${HighWay}'}
highway=Footway {set highway=footway}
highway=Cycleway {set highway=cycleway}
Mtb:scale=* & mtb:scale!=* {set mtb:scale='${Mtb:scale}'}
MTB:scale=* & mtb:scale!=* {set mtb:scale='${MTB:scale}'}
MTB:Scale=* & mtb:scale!=* {set mtb:scale='${MTB:Scale}'}
Mtb:Scale=* & mtb:scale!=* {set mtb:scale='${Mtb:Scale}'}

### Convert random typos
higwhay=* & highway!=* {set highway='${higwhay}'}
higwhey=* & highway!=* {set highway='${higwhey}'}
highwey=* & highway!=* {set highway='${highwey}'}
mbt:scale=* & mtb:scale!=* {set mtb:scale='${mbt:scale}'}
hiway=* & highway!=* {set highway='${hiway}'}
footwey=* & highway!=* {set footway='${footwey}'}
fotway=* & highway!=* {set footway='${fotway}'}
highway=residental {set highway=residential}



###################################################################
### highway conversions, simplification etc.
###################################################################

highway=pedestrian & bicycle!=no {set highway=footway}
highway=bridleway & bicycle!=no {set highway=footway}
highway=living_street {set highway=residential}
highway=bicycle_road {set highway=cycleway}
highway=cycleway {delete mtb:scale}
highway=service [0x10613 road_class=0 resolution 22-24]
# for some reason highway=service wasn't working with the internal type address...

###################################################################
### mtb:scale styles for paths/tracks/footways
###################################################################
###Paths colored based on mtb:scale or cycleway key. Solid color lines for zoomed out view to reduce clutter, poitn is to see what kind of paths are (/have been tagged) where.

### Forbidden paths/tracks/footways, must be before general mtb:scale rules (?). Disappear from view when zoomed out.
highway=path & mtb:scale="-1" [0x10E10 resolution 23-24]
highway=track & mtb:scale="-1" [0x10E11 resolution 23-24]
highway=footway & mtb:scale="-1" [0x10E10 resolution 23-24]

### Wet path/track/footway marker line overlays, must be before general mtb:scale rules. Only for zoomed in view.
highway=path & mtb:scale!=* & surface=mud [0x123 road_class=0 resolution 23-24]
highway=path & mtb:scale="0-" & surface=mud [0x124 road_class=0 resolution 23-24]
highway=path & mtb:scale=0 & surface=mud [0x125 road_class=0 resolution 23-24]
highway=path & mtb:scale="0+" & surface=mud [0x126 road_class=0 resolution 23-24]
highway=path & mtb:scale=1 & surface=mud [0x127 road_class=0 resolution 23-24]
highway=path & mtb:scale=2 & surface=mud [0x128 road_class=0 resolution 23-24]
highway=path & mtb:scale=3 & surface=mud [0x129 road_class=0 resolution 23-24]
highway=path & mtb:scale=4 & surface=mud [0x130 road_class=0 resolution 23-24]
highway=path & mtb:scale=5 & surface=mud [0x131 road_class=0 resolution 23-24]

highway=track & mtb:scale!=* & surface=mud [0x132 road_class=0 resolution 23-24]
highway=track & mtb:scale="0-" & surface=mud [0x133 road_class=0 resolution 23-24]
highway=track & mtb:scale=0 & surface=mud [0x134 road_class=0 resolution 23-24]
highway=track & mtb:scale="0+" & surface=mud [0x135 road_class=0 resolution 23-24]
highway=track & mtb:scale=1 & surface=mud [0x136 road_class=0 resolution 23-24]
highway=track & mtb:scale=2 & surface=mud [0x137 road_class=0 resolution 23-24]
highway=track & mtb:scale=3 & surface=mud [0x138 road_class=0 resolution 23-24]
highway=track & mtb:scale=4 & surface=mud [0x139 road_class=0 resolution 23-24]
highway=track & mtb:scale=5 & surface=mud [0x140 road_class=0 resolution 23-24]

highway=footway & mtb:scale!=* & surface=mud [0x123 road_class=0 resolution 23-24]
highway=footway & mtb:scale="0-" & surface=mud [0x124 road_class=0 resolution 23-24]
highway=footway & mtb:scale=0 & surface=mud [0x125 road_class=0 resolution 23-24]
highway=footway & mtb:scale="0+" & surface=mud [0x126 road_class=0 resolution 23-24]
highway=footway & mtb:scale=1 & surface=mud [0x127 road_class=0 resolution 23-24]
highway=footway & mtb:scale=2 & surface=mud [0x128 road_class=0 resolution 23-24]
highway=footway & mtb:scale=3 & surface=mud [0x129 road_class=0 resolution 23-24]
highway=footway & mtb:scale=4 & surface=mud [0x130 road_class=0 resolution 23-24]
highway=footway & mtb:scale=5 & surface=mud [0x131 road_class=0 resolution 23-24]


### Colors for highway=path
highway=path & mtb:scale!=* [0x016 road_class=0 resolution 23-24 continue]
highway=path & mtb:scale!=* [0x016 road_class=0 resolution 18-22]
highway=path & mtb:scale="0-" [0x10E00 road_class=0 resolution 23-24 continue]
