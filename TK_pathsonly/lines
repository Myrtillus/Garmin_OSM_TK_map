highway=path & mtb:scale=* & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x310 road_class=0 resolution 23-24]
highway=path & mtb:scale=* & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x311 road_class=0 resolution 24]

highway=path & mtb:scale=0 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x312 road_class=0 resolution 23-24]
highway=path & mtb:scale=0 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x313 road_class=0 resolution 24]

highway=path & mtb:scale=1 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x314 road_class=0 resolution 23-24]
highway=path & mtb:scale=1 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x315 road_class=0 resolution 24]

highway=path & mtb:scale=2 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x316 road_class=0 resolution 23-24]
highway=path & mtb:scale=2 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x317 road_class=0 resolution 24]

highway=path & mtb:scale=3 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x318 road_class=0 resolution 23-24]
highway=path & mtb:scale=3 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x319 road_class=0 resolution 24]

highway=path & mtb:scale=4 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x320 road_class=0 resolution 23-24]
highway=path & mtb:scale=4 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x321 road_class=0 resolution 24]

highway=path & mtb:scale=5 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width>0.5 | width=*) 			[0x322 road_class=0 resolution 23-24]
highway=path & mtb:scale=5 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes & (width<=0.5) 					[0x323 road_class=0 resolution 24]


### footways: nearzoom, narrow
